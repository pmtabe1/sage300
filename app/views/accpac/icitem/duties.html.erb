<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-8">
    <h2>Inventory Control</h2>
    <ol class="breadcrumb">
      <li>
        <a href="/">Home</a>
      </li>
      <li>
        Item Inquiry
      </li>
      <li class="active">
        <b>Duties</b>
      </li>
    </ol>
	</div>
</div>

<div class="wrapper wrapper-content animated fadeIn">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>Duties</h5>
          <div class="ibox-tools">
            <a class="fullscreen-link">
              <i class="fa fa-expand"></i>
            </a>
          </div>
        </div>
        <div class="ibox-content">
          <table id="datatable" style="width:100%" class="table table-striped table-hover" data-source="<%= duties_accpac_icitem_index_url(format: "json") %>">
           <thead>
            <tr>
             <th>Item Code</th>
             <th>Description</th>
             <th>Category</th>
             <th>HTS</th>
             <th data-sortable="false">Duty</th>
             <th data-sortable="false">Duty API</th>
             <th data-sortable="false">Extra Duty</th>
             <th data-sortable="false">Anti-Dumping</th>
             <th data-sortable="false">Countervailing</th>
            </tr>
          </thead>
           <tbody>
           </tbody>
         </table>
        </div>
      </div>
    </div>
  </div>
</div>
